{
  "name": "screenshot-service",
  "version": "0.15.0",
  "description": "Screenshot service based on Puppeteer",
  "scripts": {
    "start": "serverless offline start --dontPrintOutput",
    "deploy": "sls deploy",
    "deploy:function": "sls deploy function --function png --verbose",
    "logs": "sls logs --function png --tail",
    "tsc": "tsc --noEmit --skipLibCheck",
    "lint": "eslint \"src/**\"",
    "lint:fix": "eslint \"src/**\" --fix",
    "format": "prettier --write ./src"
  },
  "dependencies": {
    "chrome-aws-lambda": "^8.0.2",
    "puppeteer-core": "^8.0.0"
  },
  "devDependencies": {
    "@babel/core": "7.13.15",
    "@babel/preset-env": "7.13.15",
    "@babel/preset-typescript": "^7.13.0",
    "@types/puppeteer-core": "^5.4.0",
    "@typescript-eslint/eslint-plugin": "^4.21.0",
    "@typescript-eslint/parser": "^4.21.0",
    "babel-jest": "^26.6.3",
    "babel-loader": "^8.2.2",
    "cross-env": "^7.0.3",
    "eslint": "^7.23.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.3.4",
    "prettier": "^2.2.1",
    "puppeteer": "^8.0.0",
    "serverless": "^2.35.0",
    "serverless-apigw-binary": "^0.4.4",
    "serverless-hooks-plugin": "^1.1.0",
    "serverless-offline": "^8.1.0",
    "serverless-webpack": "^5.4.1",
    "typescript": "^4.2.4",
    "webpack": "5.94.0",
    "webpack-node-externals": "^2.5.2"
  },
  "license": "MIT"
}
